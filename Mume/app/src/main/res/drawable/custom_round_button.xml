<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#1a1a1a" />
            <solid android:color="#0d0d0d"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#1a1a1a" />
            <gradient android:angle="-90" android:startColor="#0d0d0d" android:endColor="#1a1a1a"  />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#1a1a1a" />
            <gradient android:angle="-90" android:startColor="#0d0d0d" android:endColor="#262626"  />
        </shape>
    </item>
</selector>